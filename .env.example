# Gemini AI API Key (Required for AI assistant functionality)
GEMINI_API_KEY=your_gemini_api_key_here

# Database (Optional - if using database features)
DATABASE_URL=your_database_url

# Authentication (Optional - if using NextAuth.js)
NEXTAUTH_SECRET=your_nextauth_secret
NEXTAUTH_URL=http://localhost:3000

# Payment (Optional - if using Stripe)
STRIPE_PUBLIC_KEY=your_stripe_public_key
STRIPE_SECRET_KEY=your_stripe_secret_key

# Email (Optional - if using email services)
EMAIL_SERVER_HOST=your_email_host
EMAIL_SERVER_PORT=587
EMAIL_SERVER_USER=your_email_user